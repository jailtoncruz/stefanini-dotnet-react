{
  "ReverseProxy": {
    "Routes": {
      "vite-dev-server": {
        "ClusterId": "vite-cluster",
        "Match": {
          "Path": "{**catch-all}"
        },
        "Transforms": [{ "PathPattern": "{**catch-all}" }]
      }
    },
    "Clusters": {
      "vite-cluster": {
        "Destinations": {
          "vite-dev": {
            "Address": "http://localhost:5173/"
          }
        }
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}
